<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="0dp"
        android:left="10dp"
        android:right="0dp"
        android:bottom="10dp">

        <shape
            android:shape="rectangle">

                <solid
                    android:color="#9c4747">



                </solid>


            </shape>

    </item>

    <item
        android:top="10dp"
        android:left="10dp"
        android:right="60dp"
        android:bottom="10dp">


        <rotate
            android:fromDegrees="45"
            android:toDegrees="45">

            <shape
                android:shape="rectangle"/>

        </rotate>





    </item>

</layer-list>
